<UserControl x:Class="JsonFlier.UserControls.Tabs.TabContents.PlainText"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:JsonFlier.UserControls.Tabs.TabContents"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid Background="#FFE5E5E5">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="23*"/>
            <ColumnDefinition Width="745*"/>
        </Grid.ColumnDefinitions>
        <TextBlock x:Name="TextBlock" TextWrapping="Wrap" Text="TextBlock" Grid.Column="1" Background="{DynamicResource {x:Static SystemColors.InfoBrushKey}}"/>
    </Grid>
</UserControl>
