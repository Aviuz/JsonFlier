<Window x:Class="JsonFlier.UserControls.Configuration.BookmarksManagementWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:JsonFlier.UserControls.Configuration"
        mc:Ignorable="d"
        Title="Bookmarks" Height="450" Width="800" Closing="Window_Closing">
    <ScrollViewer>
    <DockPanel x:Name="dockPanel">
            <Button Width="32" Height="32" Margin="20" Click="AddBookmark_Click" DockPanel.Dock="Top" Background="{x:Null}" BorderBrush="{x:Null}" Foreground="{x:Null}" VerticalAlignment="Top">
                <Image Source="/JsonFlier;component/Resources/add.png"/>
            </Button>
    </DockPanel>
    </ScrollViewer>
</Window>
